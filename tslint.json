{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-plugin-prettier",
        "tslint-config-prettier",
        "tslint-react"
    ],
    "jsRules": {
        "no-unused-expression": true,
        "object-literal-sort-keys": false
    },
    "rules": {
        "prettier": true,
        "semicolon": [
            true,
            "never"
        ],
        "eofline": false,
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "jsx-double"
        ],
        "indent": true,
        "member-access": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "max-line-length": [
            true,
            150
        ],
        "member-ordering": [
            false
        ],
        "curly": false,
        "interface-name": [
            false
        ],
        "array-type": [
            false
        ],
        "no-empty-interface": false,
        "no-empty": false,
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "no-unused-expression": false,
        "one-line": [
            false
        ],
        "max-classes-per-file": false,
        "no-console": false,
        "jsx-no-multiline-js": false,
        "jsx-boolean-value": false
    }
}