{
  "scripts": {
    "start": "docker-compose up --build",
    "bootstrap": "npx lerna bootstrap",
    "format": "prettier --write \"packages/**/src/**/*.ts\"",
    "lint": "tslint -c tslint.json 'packages/*/src/**/*.ts{,x}'",
    "lint:prod": "tslint -c tslint.prod.json 'packages/*/src/**/*.ts{,x}'",
    "lint:fix": "yarn run lint --fix"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "husky": "^1.1.4",
    "lerna": "^3.10.5",
    "lint-staged": "^8.0.4",
    "prettier": "1.15.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2"
  },
  "name": "farmacias",
  "engines": {
    "node": ">=10.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "dependencies": {}
}
